    "jsdom": "^16.2.2",
    "jwt-redhat": "git+https://github.com/RedHatInsights/jwt-redhat.git#b7b00d8c4e400db9aeb7ceb18d263972e0cec66e",
    "lodash": "^4.17.15",
    "md5-file": "^5.0.0",
    "node-fetch": "^2.6.0",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-redux": "^7.2.0",
    "react-test-renderer": "^16.13.1",
    "redux": "^4.0.5",
    "urijs": "^1.19.2",
    "url-parse": "^1.4.7"
  },
  "alias": {
    "PFReactTable": "./config/patternfly-table-externals.js",
    "@patternfly/react-table": "./config/patternfly-table-externals.js",
    "customReact": "./config/react-external.js",
    "reactRedux": "./config/react-redux-external.js",
    "react-router-dom": "./config/react-router-dom-externals.js",
    "graphql": "./config/graphql-externals.js"
  },
  "insights": {
    "appname": "chrome"
  }
}
