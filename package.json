{
  "name": "insights-chrome",
  "version": "0.0.0",
  "description": "Chroming for Insights",
  "main": "index.js",
  "scripts": {
    "start": "NODE_ENV='development' npm-run-all --parallel \"build:pug -- -w\" \"build:js:watch\" \"build:sass\" \"build:sass -- -w\"",
    "build": "npm run build:js && npm run build:pug && npm run build:sass",
    "build:js:watch": "parcel watch src/js/chrome.js -d build/js",
    "build:js": "parcel build src/js/chrome.js -d build/js",
    "build:pug": "pug src/pug -o build/snippets",
    "build:sass": "node-sass src/sass/chrome.scss -o build/",
    "lint": "eslint src",
    "test": "jest",
    "verify": "npm-run-all lint build test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RedHatInsights/insights-chrome.git"
  },
  "keywords": [
    "Chrome"
  ],
  "author": "ihands@redhat.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/RedHatInsights/insights-chrome/issues"
  },
  "homepage": "https://github.com/RedHatInsights/insights-chrome#readme",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^7.2.3",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-inline-environment-variables": "^0.4.3",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "concurrently": "^3.5.1",
    "eslint": "^4.19.1",
    "eslint-config-prettier": "^2.9.0",
    "jest": "^23.1.0",
    "node-sass": "^4.9.0",
    "npm-run-all": "^4.1.3",
    "parcel": "^1.8.1",
    "pug": "^2.0.3",
    "pug-cli": "^1.0.0-alpha6"
  },
  "dependencies": {
    "@red-hat-insights/insights-frontend-components": "^0.1.12",
    "babel-core": "^6.26.3",
    "babel-preset-es2015": "^6.24.1",
    "lodash": "^4.17.10",
    "redux": "^4.0.0",
    "redux-logger": "^3.0.6"
  }
}
