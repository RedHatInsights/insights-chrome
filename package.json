    "@babel/plugin-transform-runtime": "^7.10.1",
    "@babel/preset-env": "^7.10.2",
    "@babel/preset-react": "^7.10.1",
    "@redhat-cloud-services/eslint-config-redhat-cloud-services": "0.0.0",
    "@testing-library/react": "^11.2.2",
    "@types/node": "^14.0.13",
    "assert": "^2.0.0",
    "babel-cli": "^6.26.0",
