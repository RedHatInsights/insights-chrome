{
  "name": "insights-chrome",
  "version": "0.0.0",
  "description": "Chroming for Insights",
  "engines": {
    "node": ">=15.0.0",
    "npm": ">=7.0.0"
  },
  "main": "index.js",
  "scripts": {
    "start": "NODE_ENV=development SOURCEMAPS=true npm run watch",
    "start:beta": "NODE_ENV=development SOURCEMAPS=true npm run build:js:beta -- --env noHash=true && npm run build:sass && npm run build:pug:beta -- nohash && npm-run-all --parallel watch:beta:*",
    "analyze:js": "NODE_ENV=production webpack --config config/webpack.config.js --env analyze=true --env publicPath=/apps/chrome/js/ --mode=production",
    "build": "NODE_ENV=production npm run build:js -- --mode=production && npm-run-all build:images-chrome  build:sass    clean:chrome    hash:chrome   build:pug   build:fonts",
    "build:beta": "NODE_ENV=production npm run build:js:beta -- --mode=production && npm-run-all build:images-chrome build:sass clean:chrome hash:chrome build:pug:beta build:fonts",
    "build:js": "webpack --config config/webpack.config.js --env publicPath=/apps/chrome/js/",
    "build:js:beta": "webpack --config config/webpack.config.js --env publicPath=/beta/apps/chrome/js/",
    "build:images-chrome": "mkdir -p build/assets/images && ncp static/images build/assets/images",
    "build:pug": "node ./scripts/build-pug.js insights",
    "build:pug:beta": "node ./scripts/build-pug.js insightsbeta",
    "build:sass": "node ./scripts/build-sass.js",
    "build:fonts": "mkdir -p build/assets/fonts && ncp node_modules/@patternfly/patternfly/assets/fonts build/assets/fonts",
    "clean:chrome": "rimraf build/chrome.*.css",
    "hash:chrome": "contentHash=$(md5-file build/chrome.css) && ncp build/chrome.css build/chrome.$contentHash.css && rimraf build/chrome.css",
    "fixjs": "eslint src --fix",
    "lint": "npm-run-all lint:*",
    "lint:js": "eslint src",
    "test": "jest --forceExit --detectOpenHandles",
    "test:update": "jest --updateSnapshot",
    "verify": "npm-run-all lint build test",
    "verify:beta": "npm-run-all lint build:beta test",
    "watch": "npm run build:js -- --mode=development --env noHash=true && npm run build:sass && npm run build:pug -- nohash && npm-run-all --parallel watch:*",
    "watch:images": "npm run build:images-chrome -- -w",
    "watch:js": "npm run build:js -- --watch --mode=development --env noHash=true",
    "watch:sass": "npm run build:sass -- -w",
    "watch:beta:js": "npm run build:js:beta -- --watch --mode=development --env noHash=true",
    "watch:beta:sass": "npm run build:sass -- -w",
    "nightly": "npm run build"
  },
  "jest": {
    "coverageDirectory": "./coverage/",
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/**/*Styles.js"
    ],
    "testEnvironment": "jsdom",
    "transformIgnorePatterns": [
      "<rootDir>/node_modules/(?!@redhat-cloud-services).+(js|jsx)$"
    ],
    "roots": [
      "<rootDir>/src/"
    ],
    "moduleFileExtensions": [
      "js"
    ],
    "setupFiles": [
      "<rootDir>/config/setupTests.js"
    ],
    "moduleNameMapper": {
      "\\.(css|scss)$": "identity-obj-proxy",
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/src/__mocks__/fileMock.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RedHatInsights/insights-chrome.git"
  },
  "keywords": [
    "Chrome"
  ],
  "author": "ihands@redhat.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/RedHatInsights/insights-chrome/issues"
  },
  "homepage": "https://github.com/RedHatInsights/insights-chrome#readme",
  "devDependencies": {
    "@anduh/pug-cli": "^1.0.0-alpha8",
    "@babel/core": "^7.15.0",
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.15.0",
    "@babel/preset-env": "^7.15.0",
    "@babel/preset-react": "^7.14.5",
    "@redhat-cloud-services/eslint-config-redhat-cloud-services": "^1.2.1",
    "@redhat-cloud-services/frontend-components-config-utilities": "^1.4.9",
    "@testing-library/react": "^12.0.0",
    "@testing-library/react-hooks": "^7.0.2",
    "assert": "^2.0.0",
    "babel-jest": "^27.1.0",
    "babel-loader": "^8.2.2",
    "babel-plugin-lodash": "^3.3.4",
    "babel-plugin-rewire": "^1.2.0",
    "browserify-zlib": "^0.2.0",
    "buffer": "^6.0.3",
    "clean-webpack-plugin": "^4.0.0",
    "css-loader": "^6.2.0",
    "eslint": "^7.32.0",
    "file-loader": "^6.2.0",
    "git-revision-webpack-plugin": "^5.0.0",
    "html-webpack-plugin": "^5.3.2",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.1.0",
    "jest-fetch-mock": "^3.0.3",
    "jest-mock-axios": "^4.4.0",
    "jsdom": "^17.0.0",
    "mkdir": "^0.0.2",
    "ncp": "^2.0.0",
    "node-sass-package-importer": "^5.3.2",
    "npm-run-all": "^4.1.5",
    "path-browserify": "^1.0.1",
    "pug": "^3.0.2",
    "redux-logger": "^3.0.6",
    "redux-mock-store": "^1.5.4",
    "resolve-url-loader": "^4.0.0",
    "rimraf": "^3.0.2",
    "sass": "^1.39.0",
    "sass-loader": "^12.1.0",
    "source-map-loader": "^3.0.0",
    "stream-browserify": "^3.0.0",
    "style-loader": "^3.2.1",
    "terser-webpack-plugin": "^5.2.0",
    "url": "^0.11.0",
    "webpack": "^5.51.1",
    "webpack-bundle-analyzer": "^4.4.2",
    "webpack-cli": "^4.8.0",
    "write-file-webpack-plugin": "^4.5.1"
  },
  "dependencies": {
    "@data-driven-forms/pf4-component-mapper": "^3.12.0",
    "@data-driven-forms/react-form-renderer": "^3.12.0",
    "@patternfly/patternfly": "^4.132.2",
    "@patternfly/quickstarts": "1.2.0-rc.1",
    "@patternfly/react-core": "^4.152.4",
    "@patternfly/react-icons": "^4.11.14",
    "@patternfly/react-table": "^4.29.58",
    "@patternfly/react-tokens": "^4.12.15",
    "@redhat-cloud-services/entitlements-client": "^1.0.101",
    "@redhat-cloud-services/frontend-components": "^3.4.0",
    "@redhat-cloud-services/frontend-components-notifications": "^3.2.4",
    "@redhat-cloud-services/frontend-components-utilities": "^3.2.3",
    "@redhat-cloud-services/host-inventory-client": "^1.0.101",
    "@redhat-cloud-services/keycloak-js": "0.0.3",
    "@redhat-cloud-services/rbac-client": "^1.0.101",
    "@scalprum/core": "0.0.10",
    "@scalprum/react-core": "0.1.0",
    "@sentry/browser": "^6.12.0",
    "abortcontroller-polyfill": "^1.7.3",
    "axios": "^0.21.4",
    "axios-cache-adapter": "^2.7.3",
    "axios-mock-adapter": "^1.20.0",
    "broadcast-channel": "^4.2.0",
    "classnames": "^2.3.1",
    "js-cookie": "^3.0.1",
    "js-yaml": "^4.1.0",
    "localforage": "^1.10.0",
    "lodash": "^4.17.21",
    "md5-file": "^5.0.0",
    "mini-css-extract-plugin": "^2.1.0",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-intl": "^5.20.10",
    "react-redux": "^7.2.4",
    "react-router-dom": "^5.2.1",
    "react-test-renderer": "^17.0.2",
    "redux": "^4.1.1",
    "redux-promise-middleware": "^6.1.2",
    "title-case": "^3.0.3",
    "urijs": "^1.19.7"
  },
  "alias": {
    "PFReactTable": "./config/patternfly-table-externals.js",
    "reactRedux": "./config/react-redux-external.js",
    "customReact": "./config/react-external.js",
    "PFReactCore": "./config/pf-react-core-externals.js"
  },
  "insights": {
    "appname": "chrome"
  }
}
