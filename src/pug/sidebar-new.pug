        .pf-l-split__item.ins-c-page__home-icon
            a
                i.fas.fa-home.fa-lg
        .pf-l-split__item.pf-m-main.pf-u-display-flex.pf-u-align-items-center.ins-c-navigation__header-name--loading
            .ins-c-skeleton.ins-c-skeleton__lg &nbsp;
